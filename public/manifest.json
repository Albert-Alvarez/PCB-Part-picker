{
  "manifest_version": 2,
  "name": "PCB Part picker",
  "description": "Lalilulelo",
  "version": "0.0.0",

  "browser_action": { },
  "permissions": [
    "tabs"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [{
    "js": [ "content.js"],
    "matches": [ "<all_urls>"]
   }],
  "web_accessible_resources": [
    "*.html"
  ]
}